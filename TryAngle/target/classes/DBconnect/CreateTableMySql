create table TRY_CONTACT(
cNo int auto_increment Primary key,
cID varchar(30) NOT NULL,
cType varchar(10) NOT NULL,
cName varchar(20) NOT NULL,
cTel varchar(40) NOT NULL,
cTitle varchar(100) NOT NULL,
cContext varchar(3000) NOT NULL,
cDate varchar(20)
);



create table TRY_FAQ(
Q_NUM int auto_increment Primary key,
Q_TITLE varchar(100) NOT NULL,
Q_TEXT varchar(3000) NOT NULL
);


create table TRY_FAQCONTACT(
FID varchar(25) NOT NULL,
fType varchar(15) NOT NULL,
fNAme varchar(25) NOT NULL,
fTEL varchar(25) NOT NULL,
fTITLE varchar(100) NOT NULL,
fTEXT varchar(2000) NOT NULL,
fDate varchar(20) NOT NULL
);



create table TRY_MEMBER(
MID varchar(15) primary key,
MPw varchar(200) NOT NULL,
MNAME varchar(20) NOT NULL,
MTEL varchar(50) NOT NULL,
MTEL_CHK varchar(5) default 'X'
);



create table TRY_NAVER(
MID varchar(50) primary key,
MNAME varchar(20) NOT NULL,
MTEL varchar(50) NOT NULL,
MTEL_CHK varchar(5) default 'X'
);
create table TRY_KAKAO(
MID varchar(50) primary key,
MNAME varchar(20) NOT NULL,
MTEL varchar(50) NOT NULL,
MTEL_CHK varchar(5) default 'X'
);




